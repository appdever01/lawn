		.darker-image-container {
      position: relative;
      display: inline-block;
      overflow: hidden; /* Ensure that the pseudo-element doesn't extend beyond the container */
      background: url('../../images/image_1.jpg') center/cover; /* Set the background image */
    }

    .darker-image-container::before {
      content: '';
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      background-color: rgba(0, 0, 0, 0.5); /* Adjust the alpha value for darkness (0 to 1) */
      pointer-events: none;
    }

	.service-container {
      height: 30%;
    }

    .service-content {
      height: 100%;
      background-image: url('../../images/image_1.jpg');
      background-size: cover; /* Ensure the background image covers the entire container */
